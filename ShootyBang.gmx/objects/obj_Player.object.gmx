//Make character sprite look at cursor
//Down
if (point_direction(0, 0, gamepad_axis_value(0, gp_axisrh), gamepad_axis_value(0, gp_axisrv)) &gt; 240 &amp;&amp; point_direction(0, 0, gamepad_axis_value(0, gp_axisrh), gamepad_axis_value(0, gp_axisrv)) &lt; 300 )
