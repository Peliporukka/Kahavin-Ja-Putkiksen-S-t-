sprite_index = spr_PlayerDown;
}
//DownRight
if (point_direction(0, 0, gamepad_axis_value(0, gp_axisrh), gamepad_axis_value(0, gp_axisrv)) &gt; 300 &amp;&amp; point_direction(0, 0, gamepad_axis_value(0, gp_axisrh), gamepad_axis_value(0, gp_axisrv)) &lt; 345 )
{
sprite_index = spr_PlayerDownRight;
}
//Right
if (point_direction(0, 0, gamepad_axis_value(0, gp_axisrh), gamepad_axis_value(0, gp_axisrv)) &gt; 345 || (point_direction(0, 0, gamepad_axis_value(0, gp_axisrh), gamepad_axis_value(0, gp_axisrv)) &gt; 0 &amp;&amp; point_direction(0, 0, gamepad_axis_value(0, gp_axisrh), gamepad_axis_value(0, gp_axisrv)) &lt; 15  ))
{
sprite_index = spr_PlayerRight;
}
//UpRight
if (point_direction(0, 0, gamepad_axis_value(0, gp_axisrh), gamepad_axis_value(0, gp_axisrv)) &gt; 15 &amp;&amp; point_direction(0, 0, gamepad_axis_value(0, gp_axisrh), gamepad_axis_value(0, gp_axisrv)) &lt; 60 )
{
